<?xml version="1.0" encoding="UTF-8"?>
<classpath>
	<classpathentry kind="src" path="src"/>
	<classpathentry kind="con" path="org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.debug.ui.launcher.StandardVMType/JavaSE-1.8"/>
	<classpathentry kind="src" path=".apt_generated">
		<attributes>
			<attribute name="optional" value="true"/>
		</attributes>
	</classpathentry>
	<classpathentry kind="lib" path="<%= TRUFFLE_API_JAR %>" sourcepath="<%= TRUFFLE_API_SRC %>"/>
<% `cd vm && mvn dependency:build-classpath`.lines.grep(/^[^\[]/)[0].strip.split(':').each do |path| %>
	<classpathentry kind="lib" path="<%= path %>" sourcepath="<%= path[0...-4] %>-sources.jar"/>
<% end %>
	<classpathentry combineaccessrules="false" kind="src" path="/bootcompiler"/>
	<classpathentry kind="output" path="bin"/>
</classpath>
